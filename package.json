{
  "name": "react-unit-tests",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "2.x",
    "prop-types": "15.x",
    "react": "16.x",
    "react-dom": "16.x",
    "react-router-dom": "5.x",
    "react-scripts": "3.x"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-storyshots": "^6.0.21",
    "@storybook/node-logger": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^3.4.1",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "cypress": "^5.2.0",
    "cypress-plugin-snapshots": "^1.4.4",
    "enzyme": "3.x",
    "enzyme-adapter-react-16": "1.x",
    "eslint-plugin-cypress": "^2.11.1",
    "jest-fetch-mock": "^3.0.3",
    "react-is": "^16.13.1",
    "wait-on": "^5.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "cypress:test": "cross-env BROWSER=none concurrently 'npm run start' 'npm run cypress:run' -k -s first",
    "cypress:run": "wait-on http://localhost:3000 && cypress run",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:3001"
}
